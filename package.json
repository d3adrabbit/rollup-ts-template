    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-config": "^29.4.1",
    "lint-staged": "^13.1.0",
